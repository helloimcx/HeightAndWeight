<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.HomeworkOne"
    android:versionCode="7"
    android:versionName="3.3" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/luanch"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.HomeworkOne.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.HomeworkOne.FActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.HomeworkOne.Welcome"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.HomeworkOne.Access"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.HomeworkOne.SActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.HomeworkOne.RecordActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.HomeworkOne.RecordDetail"
            android:label="@string/app_name" >
        </activity>

        <provider
            android:name="com.android.tools.fd.runtime.InstantRunContentProvider"
            android:authorities="com.example.HomeworkOne.com.android.tools.fd.runtime.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>